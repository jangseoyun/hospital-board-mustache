{{>layouts/header}}
    <div class="card text-bg-warning mb-3" style="width: 70rem;">
        {{#article}}
        <div class="card-body" style="text-align: center">
            <h5 class="card-title">{{title}}</h5>
            <h6 class="card-subtitle mb-2 text-muted">{{id}}번 게시물이 등록되었습니다 :-) </h6>
            <p class="card-text">{{contents}}</p>
            <a href="/articles/new" class="card-link">new article</a>
            <a href="/articles/list" class="card-link">list article</a>
        </div>
        {{/article}}
    </div>
{{>layouts/footer}}